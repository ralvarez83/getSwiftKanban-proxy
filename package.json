{
  "name": "getswiftkanban-prxoy",
  "version": "0.0.1",
  "description": "getswiftkanban-prxoy initialised with create-express-typescript-application.",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "cd build && node src",
    "dev": "nodemon",
    "deploy": "firebase deploy --only hosting:getswiftkanban-proxy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, json",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "exec": "ts-node ./src/index.ts"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "firebase-functions": "^3.13.1",
    "firebase-functions-helper": "^0.10.0",
    "fs": "0.0.1-security",
    "helmet": "^4.4.1",
    "http-proxy-middleware": "^1.0.6",
    "morgan": "^1.10.0"
  }
}
